"line":43,"column":12},"source":"js/index.js","original":{"line":55,"column":12}},{"generated":{"line":43,"column":13},"source":"js/index.js","original":{"line":55,"column":13},"name":"revised"},{"generated":{"line":43,"column":20},"source":"js/index.js","original":{"line":55,"column":20}},{"generated":{"line":43,"column":21},"source":"js/index.js","original":{"line":55,"column":21}},{"generated":{"line":44,"column":0},"source":"js/index.js","original":{"line":56,"column":0}},{"generated":{"line":44,"column":1},"source":"js/index.js","original":{"line":56,"column":1}},{"generated":{"line":44,"column":3},"source":"js/index.js","original":{"line":56,"column":3}},{"generated":{"line":44,"column":4},"source":"js/index.js","original":{"line":56,"column":4}}],"sources":{"js/index.js":"import data from \"./data.json\";\nimport Row from \"./Row\";\nconst productsDiv = document.querySelector(\"#products\");\n\nconst renderRows = function (items) {\n  const domNodes = items\n    .map(({ product, cost, totalCost = \"N/A\" }) =>\n      Row(product, cost, totalCost)\n    )\n    .join(\"\");\n\n  productsDiv.innerHTML = domNodes;\n};\n\nconst totalCost = function (tax, shipping, cost) {\n  return (Number(tax) / 100) * Number(cost) + Number(cost) + Number(shipping);\n};\n\n// smartphones and catfood are taxed at 12%\n// televisions are taxed at 18%\n// Default shipping rates (if absent in dataset): Pet food (2.5), Smartphones (5), Televisions (15)\n\nconst prepCurry = (fn) =>\n  function curry(...args) {\n    return args.length === fn.length\n      ? fn(...args)\n      : (...newArgs) => curry(...args, ...newArgs);\n  };\n\nconst totalCostCurr = prepCurry(totalCost);\n\nconst tax = {\n  tax12: totalCostCurr(12),\n  tax18: totalCostCurr(18),\n};\n\nconst shipping = {\n  petfood: tax.tax12(2.5),\n  smartphones: tax.tax12(5),\n  televisions: tax.tax18(15),\n};\n\n(() => {\n  const revised = data.map((e) => {\n    return {\n      ...e,\n      totalCost:\n        e.shipping === null\n          ? shipping[e.category](e.cost)\n          : [\"petfood\", \"smartphones\"].includes(e.category)\n          ? tax.tax12(e.shipping)(e.cost)\n          : tax.tax18(e.shipping)(e.cost),\n    };\n  });\n  renderRows(revised);\n})();\n"},"lineCount":null}},"error":null,"hash":"f0309b693b042f5a3a03efadc78e640b","cacheData":{"env":{}}}